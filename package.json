{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build:prod",
    "clean": "trash dist/assets",
    "coverage": "npm run test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "tsc && npm run lint:file",
    "lint:file": "tslint --project tsconfig.json",
    "test": "jest",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watch",
    "server": "npm run server:dev",
    "server:dev": "cross-env HOT_MODULES=true TS_NODE_PROJECT=tsconfig.base.json webpack-dev-server --port 3000",
    "server:story": "cross-env STORYBOOK=true TS_NODE_PROJECT=tsconfig.base.json start-storybook -p 9001",
    "build:dev": "npm run clean && cross-env TS_NODE_PROJECT=tsconfig.base.json webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build:dev",
    "build:story": "cross-env STORYBOOK=true NODE_ENV=production TS_NODE_PROJECT=tsconfig.base.json build-storybook -o dist/story"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@jupl/ts": "0.0.5",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^11.13.7",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.14",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-env": "^1.13.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.1.5",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "styled-components": "^4.4.0",
    "trash-cli": "^2.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "wcb": "^0.15.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
